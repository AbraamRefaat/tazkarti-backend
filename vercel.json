{
  "version": 2,
  "builds": [
    {
      "src": "api/events.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/events",
      "dest": "/api/events.js"
    },
    {
      "src": "/api/events/music",
      "dest": "/api/events.js"
    }
  ],
  "functions": {
    "api/events.js": {
      "maxDuration": 60,
      "memory": 1024
    }
  }
}
